  overflow: hidden;\r\n\r\n    @media screen and (max-width: 768px) {\r\n        width: 90%;\r\n    }\r\n}\r\n\r\nul {\r\n    list-style: none;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n}\r\n\r\n/* Night Light Toggle Button Styles */\r\n.night-light-toggle-button {\r\n    .night-light-checkbox {\r\n        position: absolute;\r\n        visibility: hidden;\r\n\r\n        &:checked+.night-light-label .night-light-ball {\r\n            transform: translateX(28px);\r\n        }\r\n    }\r\n\r\n    .night-light-label {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        width: 64px;\r\n        height: 32px;\r\n        background-color: rgb(18, 18, 25);\r\n        color: $white-color;\r\n        padding: 4px 6px;\r\n        border-radius: 64px;\r\n        cursor: pointer;\r\n\r\n        .fa-moon,\r\n        .fa-sun {\r\n            font-size: 20px;\r\n        }\r\n\r\n        .night-light-ball {\r\n            display: inline-block;\r\n            position: absolute;\r\n            width: 25px;\r\n            height: 25px;\r\n            background-color: $primary-color;\r\n            border-radius: 50%;\r\n            transition: transform 0.3s linear;\r\n        }\r\n    }\r\n}\r\n\r\n/* Menu Toggle Button Styles */\r\n.hamburger-menu-button {\r\n    height: 32px;\r\n    width: 32px;\r\n    cursor: pointer;\r\n\r\n    .fa-bars {\r\n        font-size: 32px;\r\n    }\r\n}\r\n\r\n/* Typography Styles */\r\n.extra-large-text {\r\n    font-size: $extra-large-font;\r\n\r\n    @media screen and (max-width: 768px) {\r\n        font-size: $large-font;\r\n    }\r\n}\r\n\r\n.large-text {\r\n    font-size: $large-font;\r\n\r\n    @media screen and (max-width: 768px) {\r\n        font-size: $medium-font;\r\n    }\r\n}\r\n\r\n.medium-text {\r\n    font-size: $medium-font;\r\n\r\n    @media screen and (max-width: 768px) {\r\n        font-size: $regular-font;\r\n    }\r\n}\r\n\r\n.regular-text {\r\n    font-size: $regular-font;\r\n\r\n    @media screen and (max-width: 768px) {\r\n        font-size: $small-font;\r\n    }\r\n}\r\n\r\n.small-text {\r\n    font-size: $small-font;\r\n}\r\n\r\n.primary-text {\r\n    color: $primary-color;\r\n}\r\n\r\n.secondary-text {\r\n    color: $secondary-color;\r\n}\r\n\r\n.bold-text {\r\n    font-weight: bold;\r\n}","styles\\main.scss":"@import './_variables';\r\n@import './_common';\r\n\r\n#header {\r\n    .top-navbar {\r\n        height: 72px;\r\n\r\n        &>.container {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n        }\r\n\r\n        .icon {\r\n            font-size: $large-font;\r\n            font-weight: bold;\r\n            padding: 0.5rem 0;\r\n            cursor: pointer;\r\n        }\r\n\r\n        .nav-items {\r\n            display: flex;\r\n            justify-content: space-evenly;\r\n            align-items: center;\r\n\r\n            .nav-item {\r\n                margin-left: 16px;\r\n            }\r\n        }\r\n\r\n        .menu-options {\r\n            position: absolute;\r\n            top: 64px;\r\n            right: 10%;\r\n            z-index: 1;\r\n            width: 180px;\r\n            height: 280px;\r\n            padding: 8px 0;\r\n            color: $white-color;\r\n            background-color: $secondary-color;\r\n            opacity: 0.9;\r\n\r\n            @media screen and (max-width: 768px) {\r\n                right: 5%;\r\n            }\r\n\r\n            .menu-option {\r\n                font-size: $small-font;\r\n                padding: 12px 20px;\r\n                cursor: pointer;\r\n\r\n                &:hover {\r\n                    font-weight: bold;\r\n                    color: $primary-color;\r\n                    transform: translateX(10%);\r\n                    transition: all 0.3s linear;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    #hero {\r\n        position: relative;\r\n        height: 584px;\r\n        overflow: hidden;\r\n\r\n        @media screen and (max-width: 768px) {\r\n            height: 520px;\r\n        }\r\n\r\n        &>.container {\r\n            position: absolute;\r\n            top: 10%;\r\n            left: 10%;\r\n            height: 500px;\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: space-between;\r\n\r\n            @media screen and (max-width: 768px) {\r\n                top: 30%;\r\n                left: 50%;\r\n                transform: translate(-50%, -30%);\r\n                text-align: center;\r\n                width: 90%;\r\n            }\r\n\r\n            .hero-bio {\r\n                margin-top: 48px;\r\n\r\n                .more-info {\r\n                    font-weight: bold;\r\n                    padding: 12px;\r\n                    border: 2px solid $primary-color;\r\n                    cursor: pointer;\r\n                }\r\n            }\r\n\r\n            .hero-footer {\r\n                margin-bottom: 28px;\r\n\r\n                .social-links {\r\n                    font-weight: bold;\r\n                    font-size: $large-font;\r\n\r\n                    .link {\r\n                        padding: 8px 12px;\r\n                        cursor: pointer;\r\n\r\n                        &:hover {\r\n                            color: $primary-color;\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        img {\r\n            display: inline-block;\r\n            position: absolute;\r\n            z-index: -1;\r\n            top: -6%;\r\n            right: -12%;\r\n            width: 40%;\r\n            height: auto;\r\n            max-height: 300%;\r\n            border-radius: 50%;\r\n            opacity: 0.9;\r\n\r\n            @media screen and (max-width: 768px) {\r\n                top: -12%;\r\n                right: -25%;\r\n                width: 125%;\r\n                border-radius: 0;\r\n                opacity: 0.1;\r\n            }\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"dfd8fdcd205e286a332584e74d40d3fa","cacheData":{"env":{}}}